<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.yasmine.mascotasdb2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="mx.yasmine.mascotasdb2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mx.yasmine.mascotasdb2.activities.FavouritesActivity"
            android:parentActivityName="mx.yasmine.mascotasdb2.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="mx.yasmine.mascotasdb2.MainActivity" />
        </activity>
        <activity android:name="mx.yasmine.mascotasdb2.activities.ContactoActivity" />
        <activity android:name="mx.yasmine.mascotasdb2.activities.AboutActivity" />
        <activity android:name="mx.yasmine.mascotasdb2.activities.MailConfirmActivity"></activity>
    </application>

</manifest>